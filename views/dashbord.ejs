</div>
<div>
    <center>
    <p>

        <button style=" font-size: 20px;" class="btn btn-primary" type="button" data-bs-toggle="collapse" data-bs-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
          POST JOB
        </button>
      </p>
      <div style="width: 50%;font-size: 20px;" class="collapse" id="collapseExample">
        <div class="card card-body">
          <!-- Some placeholder content for the collapse component. This panel is hidden by default but revealed when the user activates the relevant trigger. -->
          <form  action='/dashbord' method ='post'>
            <div class="form-group">
              <label for="exampleInputEmail1">Company Name</label>
              <input type="text" name='companyname' class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Eg :<%=Name %> " required>
              <small id="emailHelp" class="form-text text-muted">Mention Only Your Comapany .</small>
            </div>
            <div class="form-group">
              <label for="exampleInputPassword1">Position</label>
              <input type="text" class="form-control" name='position' id="exampleInputPassword1" placeholder="Eg: Android Developer" required>
            </div>
            <div class="form-group">
                <label for="example">CTC</label>
                <input type="text" class="form-control" name='ctc' id="example" placeholder="Eg: 10 LPA" required>
              </div>
            
            <button style="width: 80px;" id="hari" data-bs-toggle="collapse" data-bs-target="#collapseExample" type="submit" aria-expanded="false" class="btn btn-primary">Submit</button>
          </form>
        
        </div>
      </div>
    </center>
</div><br><br><hr>
<div>
  <center>
    <h1 style="font-size:30px;">Jobs Posted By Your Company <%= Name %></h1>
    <hr>
    <table style="width: 60%;font-size: 20px;border: 1px solid black;" class="table">
        <thead>
          <tr>
            <th scope="col">#</th>
            <th scope="col">Company Name</th>
            <th scope="col">Position</th>
            <th scope="col">CTC</th>
            
          </tr>
          <% var i=0; %>
          <% companysignupemail.forEach(function (item) { %>
            <% if(Name===item.companyname){%>
              <% i=i+1 %>
             
            
               <tr>
                 <td>
                   <%= i%>
                 </td>
                 <td><%=item.companyname%></td>
                 <td><%=item.position%></td>
                 <td><%=item.ctc%></td>
               </tr>
               
            
           <%}%>
             
           <% }); %>
        
        </thead>
  </table>
</center>

    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
